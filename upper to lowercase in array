#include<iostream>
#include<bits/stdc++.h>
using namespace std;

int main(){

//upper to lowercase
    string s;
    cin>>s;
    for(int i=0; i<s.size();i++)
    {
        if(s[i]>='A'&& s[i]<='Z')
        {
           s[i]+=32;
        }
    }

    for(int i=0; i<s.size();i++)
        {cout<<s[i];}

    return 0;

}
